%% $mode_msg: rsync ${rsync_opts[*]} $from $to
%% From: $(pwd)
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
EOF

    rsync "${rsync_opts[@]}" "$from" "$to"
}


